{
  "name": "DocFX",
  "id": "docfx",
  "version": "1.0.2",
  "description": "Installs docfx tools",
  "customizations": {
    "vscode": {
      "extensions": [
         "yzhang.markdown-all-in-one"
       ]
    }
  },
  "containerEnv": {
     "PATH": "/home/vscode/.dotnet/tools:${PATH}"
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils"
  ]    
}
